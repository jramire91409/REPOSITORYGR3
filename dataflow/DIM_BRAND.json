{
	"name": "DIM_BRAND",
	"properties": {
		"description": "Carga de la DIM_BRAND",
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "T_STG_E_DATOS_DE_PRODUCTO",
						"type": "DatasetReference"
					},
					"name": "STGDATOSPRODUCTO"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "T_DIM_PRODUCT",
						"type": "DatasetReference"
					},
					"name": "DIMBRAND"
				}
			],
			"transformations": [
				{
					"name": "SELECTBRAND"
				},
				{
					"name": "Modificarfila1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          product_id as string,",
				"          category_id as string,",
				"          Column3 as string,",
				"          category_code as string,",
				"          beauty_category as string,",
				"          brand as string,",
				"          product_desc as string,",
				"          category_desc as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> STGDATOSPRODUCTO",
				"STGDATOSPRODUCTO select(mapColumn(",
				"          product_id,",
				"          category_id,",
				"          Column3,",
				"          category_code,",
				"          beauty_category,",
				"          brand,",
				"          product_desc,",
				"          category_desc",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SELECTBRAND",
				"SELECTBRAND alterRow(upsertIf(isNull(brand))) ~> Modificarfila1",
				"Modificarfila1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          ID_PRODUCT as integer,",
				"          ID_CATEGORY as integer,",
				"          DESCRIPTION as string,",
				"          BEAUTY_CATEGORY as string,",
				"          ID_BRAND as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:false,",
				"     updateable:false,",
				"     upsertable:true,",
				"     keys:['brand'],",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> DIMBRAND"
			]
		}
	}
}