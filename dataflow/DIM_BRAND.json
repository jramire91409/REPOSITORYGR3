{
	"name": "DIM_BRAND",
	"properties": {
		"description": "Carga de la DIM_BRAND",
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "T_STG_E_DATOS_DE_PRODUCTO",
						"type": "DatasetReference"
					},
					"name": "STGDATOSPRODUCTO"
				},
				{
					"dataset": {
						"referenceName": "T_DIM_BRAND",
						"type": "DatasetReference"
					},
					"name": "TBRAND"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "T_DIM_BRAND",
						"type": "DatasetReference"
					},
					"name": "DIMBRAND"
				}
			],
			"transformations": [
				{
					"name": "SELECTBRAND"
				},
				{
					"name": "aggregate1"
				},
				{
					"name": "Modificarfila1"
				},
				{
					"name": "exists1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          brand as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> STGDATOSPRODUCTO",
				"source(output(",
				"          ID_BRAND as integer,",
				"          DESC_BRAND as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> TBRAND",
				"STGDATOSPRODUCTO select(mapColumn(",
				"          brand",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> SELECTBRAND",
				"SELECTBRAND aggregate(groupBy(brand),",
				"     COUNT = count()) ~> aggregate1",
				"TBRAND alterRow(upsertIf(isNull(brand))) ~> Modificarfila1",
				"aggregate1, TBRAND exists(brand == DESC_BRAND,",
				"     negate:true,",
				"     broadcast: 'auto')~> exists1",
				"Modificarfila1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          ID_BRAND as integer,",
				"          DESC_BRAND as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:true,",
				"     upsertable:true,",
				"     keys:['DESC_BRAND'],",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          ID_BRAND,",
				"          DESC_BRAND = brand",
				"     )) ~> DIMBRAND"
			]
		}
	}
}