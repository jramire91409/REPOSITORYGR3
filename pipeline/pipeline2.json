{
	"name": "pipeline2",
	"properties": {
		"activities": [
			{
				"name": "STAGE_ECOM_DATOS_DE_PRODUCTO",
				"description": "Datos de producto desde el archivo \"datos de producto.xlsx\"",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ExcelSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "SqlServerSink",
						"writeBehavior": "upsert",
						"upsertSettings": {
							"useTempDB": true,
							"keys": [
								"ID_PRODUCT"
							]
						},
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "product_id",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "ID_PRODUCT",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"source": {
									"name": "category_id",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "ID_CATEGORY",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"source": {
									"name": "product_desc",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "DESCRIPTION",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "beauty_category",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "BEAUTY_CATEGORY",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "brand",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "ID_BRAND",
									"type": "Int32",
									"physicalType": "int"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "DATOS_DE_PRODUCTO_XLSX",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DIM_PRODUCT",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}